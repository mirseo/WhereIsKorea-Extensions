{
  "manifest_version": 3,
  "name": "koreaSelector",
  "version": "1.0",
  "description": "회원가입 창에서 대한민국을 자동으로 찾아주는 확장프로그램입니다!",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "koreaSelector"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}